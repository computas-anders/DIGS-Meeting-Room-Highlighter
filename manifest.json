{
  "manifest_version": 3,
  "name": "DIGS Meeting Room Highlighter",
  "description": "Highlights rooms which support Google Meet",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://members.meshcommunity.com/*"],
      "run_at": "document_end",
      "js": ["scripts/highlight.js"]
    }
  ]
}